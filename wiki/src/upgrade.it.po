#, no-wrap
msgid ""
"<div class=\"row\">\n"
"  <div class=\"col-md-6\">\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    <h3>Upgrade from another Tails</h3>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <p>If you know someone of trust who already did the upgrade, you can\n"
"    upgrade your Tails by cloning from it.</p>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    <h4>You need:</h4>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <ul>\n"
"      <li>Your USB stick</li>\n"
"      <li>Another USB stick with Tails up-to-date</li>\n"
"      <li>10 minutes to upgrade</li>\n"
"    </ul>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    [[\n"
"      <div class=\"panel panel-primary\">\n"
"        <div class=\"panel-heading\">\n"
"          <h3 class=\"panel-title\">Upgrade from another Tails</h3>\n"
"        </div>\n"
"      </div>\n"
"    |upgrade/clone/overview]]\n"
"  </div>\n"
"  <div class=\"col-md-6\">\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    <h3>Upgrade inside Tails</h3>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <ul>\n"
"      <li>Your USB stick</li>\n"
"      <li>Another empty USB stick <small>(at least 4 GB)</small></li>\n"
"      <li>30&ndash;60 minutes to download Tails <small>[[!inline "
"pages=\"inc/stable_i386_iso_size\" raw=\"yes\"]]</small></li>\n"
"      <li>20 minutes to upgrade</li>\n"
"    </ul>\n"
msgstr ""

#. type: Plain text
#, no-wrap

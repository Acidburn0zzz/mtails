#~ "   <span class=\"application\">Universal USB Installer</span> erneut.</p>\n"
#~ "   </div>\n"

#~ msgid "   [[!img install/inc/screenshots/uui_format.png link=\"no\" alt=\"\"]]\n"
#~ msgstr "   [[!img install/inc/screenshots/uui_format.png link=\"no\" alt=\"\"]]\n"

#~ msgid "   </div>\n"
#~ msgstr "   </div>\n"

